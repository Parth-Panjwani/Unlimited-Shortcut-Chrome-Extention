{
  "manifest_version": 3,
  "name": "Unlimited Shortcuts New Tab",
  "description": "Replaces the default new tab with a custom shortcuts dashboard.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
